

<h3>Welcome, <%= user.name %></h3>

<table width="50%">
    <tr>
        <td>
            <img src="/user/<%= user.image %>" alt="<%= user.image %>">
        </td>
    </tr>
    <tr>
        <td>Email</td>
        <td>
            <%= user.email %>
        </td>
    </tr>
    <tr>
        <td>Mobile</td>
        <td>
            <%= user.mobile %>
        </td>
    </tr>
</table>
<br><br>
<a href="user/logout">Logout</a><br><br>
<a href="/edit?id=<%= user._id %>"> Edit </a>

